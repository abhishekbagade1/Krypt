{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true
       
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended",
         "airbnb-base",
         "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"

         
        // migth be in this
    ],
//     "rules": { 
//         "prettier/prettier" : "error" 
//     }
//

"rules": {
    "react/react-in-jsx-scope": "off",
    "prettier/prettier": 
    [ "error",
      {
        "endOfLine": "auto"}
    ]
  }
}

